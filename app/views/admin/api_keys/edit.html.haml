.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_api_keys_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_api_key_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_api_key_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      ApiKey
    .inner
      = form_for @api_key, :url => admin_api_key_path(@api_key), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
