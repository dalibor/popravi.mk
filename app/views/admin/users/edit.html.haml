.block
  .secondary-navigation
    %ul.wat-cf
      = render :partial => 'list'
      %li= link_to t("web-app-theme.new", :default => "New"), new_admin_user_path
      %li.active= link_to t("web-app-theme.edit", :default => "Edit"), edit_admin_user_path
  .content
    %h2.title #{ t("web-app-theme.edit", :default => "Edit") } User
    .inner
      = form_for @user, :url => admin_user_path(@user), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')
